{
  "name": "web3d-explorer",
  "version": "0.1.0",
  "description": "3D web visualization library with TDD-driven testing framework for Three.js applications",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:suite": "cd three-test-suite && npm test",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "keywords": [
    "three.js",
    "3d",
    "visualization",
    "testing",
    "tdd",
    "puppeteer",
    "gis",
    "cesium"
  ],
  "author": "usaganikki",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "puppeteer": "^22.0.0",
    "three": "^0.163.0",
    "@react-three/fiber": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}